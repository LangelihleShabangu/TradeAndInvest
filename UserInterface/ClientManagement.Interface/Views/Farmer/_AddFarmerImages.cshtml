                EntityName: $("#EntityName").val(),
                LegalEntityTypeId: $("#LegalEntityTypeId").val(),
                NumberofDirectors: $("#NumberofDirectors").val(),
                CoRegNo: $("#CoRegNo").val(),
                TaxRegNo: $("#TaxRegNo").val(),
                VatRegNo: $("#VatRegNo").val(),
                Yearsinexistance: $("#Yearsinexistance").val()
            },
            url: '@Url.Action("UpdateTradingDetails", "Farmer")',
            success: function (data) {
                onComplete();
                $("#kt_toast_toggle_1").click();
                window.location.href = '@Url.Action("FarmerDescriptionResources", "Farmer")' + '?FarmerId=' + '@Model.FarmerId';
