<script>
    function SubmitInfrustructure() {
        onBegin();
        $.ajax({
            cache: false,
            type: "GET",
            data: {
                FarmInfrastuctureId: '@Model.FarmInfrastuctureId',
                FarmerId: '@Model.FarmerId',
                Shreds: $("#Shreds").val(),
                ShredsConditions: $("#ShredsConditions").val(),
                ProcessingFacilities: $("#ProcessingFacilities").val(),
                ProcessingFacilitiesConditions: $("#ProcessingFacilitiesConditions").val(),
                Packhouse: $("#Packhouse").val(),
                StorageFacilities: $("#StorageFacilities").val(),
                StorageFacilitiesConditions: $("#StorageFacilitiesConditions").val(),
                Office: $("#Office").val(),
                OfficeConditions: $("#OfficeConditions").val(),
                Storage: $("#Storage").val(),
                StorageConditions: $("#StorageConditions").val(),
                Fencing: $("#Fencing").val(),
                FencingConditions: $("#FencingConditions").val(),
                Livestock: $("#Livestock").val(),
                LivestockConditions: $("#LivestockConditions").val(),
                LivestockInfrustructure: $("#LivestockInfrustructure").val(),
                LivestockInfrustructureCondition: $("#LivestockInfrustructureCondition").val(),
                FishTunnels: $("#FishTunnels").val(),
                FishTunnelsConditions: $("#FishTunnelsConditions").val(),
                VegetableTunnels: $("#VegetableTunnels").val(),
                VegetableTunnelsConditions: $("#VegetableTunnelsConditions").val(),
                WaterTanks: $("#WaterTanks").val(),
                WaterTanksConditions: $("#WaterTanksConditions").val(),
                FarmInfrastuctureOther: $("#FarmInfrastuctureOther").val(),
                FarmInfrastuctureOtherConditions: $("#FarmInfrastuctureOtherConditions").val(),
                AccesstoWater: $("#AccesstoWater").val(),
                SourceofWater: $("#SourceofWater").val(),
                AccesstoPowerSupply: $("#AccesstoPowerSupply").val(),
                SourceofPowerSupply: $("#SourceofPowerSupply").val(),
                NetworkCoverage: $("#NetworkCoverage").val(),
                NetworkCoverageCondition: $("#NetworkCoverageCondition").val(),
                AvailabilityofSewerageServices: $("#AvailabilityofSewerageServices").val(),
                SewerageServicesCondition: $("#SewerageServicesCondition").val()
            },
            url: '@Url.Action("UpdateFarmInfrastucture", "Farmer")',
            success: function (data) {
                onComplete();
                $("#kt_toast_toggle_1").click();
                window.location.href = '@Url.Action("FarmerSocioEconomic", "Farmer")' + '?FarmerId=' + '@Model.FarmerId';
            }
        });
    }
</script>

<script type="text/javascript">
    function onBegin() {
        $("#wait").css("display", "block");
    }
    function onComplete() {
        $("#wait").css("display", "none");
    }
</script>



